//import java.util.*;
import java.lang.System;

public class Test {

	public static void main(String[] args) 
	{
		Player eric = new Player("Eric","Earth");
		Territory[][] map = new Territory[5][5];
		
		for(int a = 0;a<map.length;a++)
		{
			for(int b = 0; b< map.length;b++)
			{
				int[] co = {a,b};
				map[a][b] = new Territory("["+a+","+b+"]",co);
			}
		}
		
		for(int a=0;a<5;a++)
		{
			Unit zergling = new Unit("Zergling",35,5,0,eric);
			zergling.setID(a);
			zergling.setTerritory(map[a][a]);
			eric.addUnit(zergling);
		}
		
		for(int id:eric.getArmy().keySet())
			System.out.println(id+" = "+eric.getArmy().get(id));
		System.out.println(eric.getOccupiedTerritories());
		Unit zergling = new Unit("Zergling",35,5,0,eric);
		zergling.setID(5);
		zergling.setTerritory(map[3][3]);
		zergling = new Unit("Zergling",35,5,0,eric);
		zergling.setID(6);
		zergling.setTerritory(map[3][2]);
		System.out.println(eric.getOccupiedTerritories());
/*		for(Territory a: eric.getOccupiedTerritories().keySet())
			System.out.println(a);*/
	

	}

}
